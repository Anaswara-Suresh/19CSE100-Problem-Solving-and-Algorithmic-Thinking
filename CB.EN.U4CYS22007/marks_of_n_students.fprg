            <output expression="&quot; ROLL NO : 4  &quot;" newline="True"/>
            <output expression="&quot; TOTAL MARK OF ROLL NO 4 = &quot;&amp; total4" newline="True"/>
            <declare name="avg4" type="Real" array="False" size=""/>
            <assign variable="avg4" expression="total4/5"/>
            <output expression="&quot; AVERAGE MARK OF ROLL NO 4 = &quot;&amp;avg4" newline="True"/>
            <comment text="avg and total is found"/>
            <output expression="&quot; marks of roll no 5&quot;" newline="True"/>
            <for variable="indexsub" start="1" end="5" direction="inc" step="1">
                <comment text="marks of 5th student"/>
                <output expression="&quot; enter maek for subject [&quot;&amp;indexsub&amp;&quot;]&quot;" newline="True"/>
                <input variable="mark"/>
                <assign variable="mark5[indexsub]" expression="mark"/>
            </for>
            <declare name="total5" type="Real" array="False" size=""/>
