            <output expression="&quot; ROLL NO : 5 &quot;" newline="True"/>
            <output expression="&quot; TOTAL MARK OF ROLL NO 2 = &quot;&amp; total2" newline="True"/>
            <declare name="avg5" type="Real" array="False" size=""/>
            <assign variable="avg5" expression="total5/5"/>
            <output expression="&quot; AVERAGE MARK OF ROLL NO 5 = &quot;&amp;avg5" newline="True"/>
            <comment text="avg and total is found"/>
        </body>
    </function>
